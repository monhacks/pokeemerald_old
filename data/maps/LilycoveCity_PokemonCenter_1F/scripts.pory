raw `
LilycoveCity_PokemonCenter_1F_MapScripts:: @ 821C5B2
	map_script MAP_SCRIPT_ON_TRANSITION, LilycoveCity_PokemonCenter_1F_OnTransition
	map_script MAP_SCRIPT_ON_RESUME, CableClub_OnResume
	.byte 0

LilycoveCity_PokemonCenter_1F_OnTransition: @ 821C5BD
	setrespawn HEAL_LOCATION_LILYCOVE_CITY
	goto LilycoveCity_PokemonCenter_1F_EventScript_SetLilycoveLadyGfx
	end

@ SetLilycoveLadyGfx returns TRUE if its the Contest Lady
LilycoveCity_PokemonCenter_1F_EventScript_SetLilycoveLadyGfx:: @ 821C5C6
	special SetLilycoveLadyGfx
	compare VAR_RESULT, FALSE
	goto_if_eq LilycoveCity_PokemonCenter_1F_EventScript_HideContestLadyMon
	compare VAR_RESULT, TRUE
	goto_if_eq LilycoveCity_PokemonCenter_1F_EventScript_ShowContestLadyMon
	end

LilycoveCity_PokemonCenter_1F_EventScript_HideContestLadyMon:: @ 821C5E0
	setflag FLAG_HIDE_LILYCOVE_POKEMON_CENTER_CONTEST_LADY_MON
	end

LilycoveCity_PokemonCenter_1F_EventScript_ShowContestLadyMon:: @ 821C5E4
	clearflag FLAG_HIDE_LILYCOVE_POKEMON_CENTER_CONTEST_LADY_MON
	end

@ VAR_0x800B is the Nurse's object event id
LilycoveCity_PokemonCenter_1F_EventScript_Nurse:: @ 821C5E8
	setvar VAR_0x800B, 1
	call Common_EventScript_PkmnCenterNurse
	waitmessage
	waitbuttonpress
	release
	end

LilycoveCity_PokemonCenter_1F_EventScript_Boy:: @ 821C5F6
	msgbox LilycoveCity_PokemonCenter_1F_Text_HowManyKindsOfPokemon, MSGBOX_NPC
	end

LilycoveCity_PokemonCenter_1F_EventScript_Maniac:: @ 821C5FF
	lock
	faceplayer
	goto_if_set FLAG_BADGE07_GET, LilycoveCity_PokemonCenter_1F_EventScript_ManiacBadTeamGone
	msgbox LilycoveCity_PokemonCenter_1F_Text_HeardAboutRottenScoundrels, MSGBOX_DEFAULT
	release
	end

LilycoveCity_PokemonCenter_1F_EventScript_ManiacBadTeamGone:: @ 821C614
	msgbox LilycoveCity_PokemonCenter_1F_Text_HaventSeenRottenScoundrels, MSGBOX_DEFAULT
	release
	end

LilycoveCity_PokemonCenter_1F_Text_HowManyKindsOfPokemon: @ 821C61E
	.string "I wonder how many kinds of POKéMON\n"
	.string "there are in the world.\p"
	.string "It'd be great to cross seas and\n"
	.string "trade POKéMON with people far away.$"

LilycoveCity_PokemonCenter_1F_Text_HeardAboutRottenScoundrels: @ 821C69D
	.string "I've been hearing about some rotten\n"
	.string "scoundrels who steal POKéMON and rip\l"
	.string "off METEORITES.$"

LilycoveCity_PokemonCenter_1F_Text_HaventSeenRottenScoundrels: @ 821C6F6
	.string "Those rotten scoundrels who steal\n"
	.string "POKéMON and rip off METEORITES…\p"
	.string "I haven't seen them around recently.$"
`

script MystCity_PokemonCenter_LarsEvent {
	lock
	msgbox(format("Lars: {COLOR BLUE}Thank you Nurse Joy!\nHave a good day!"), MSGBOX_AUTOCLOSE)
	delay(10)
	faceplayer
	delay(10)
	applymovement(4, Common_Movement_ExclamationMark)
	playse(SE_PIN)
	waitse
	waitmovement(0)
	msgbox(format("Lars: {COLOR BLUE}Hey there champ!\pNice seeing you again!\pThought I would just stop by good old {COLOR GREEN}Myst City{COLOR BLUE}!\pDid you end up getting that book from {COLOR GREEN}Enclava Tower{COLOR BLUE}?"), MSGBOX_YESNO)
	if (var(VAR_RESULT) == TRUE) {
		msgbox(format("Lars: {COLOR BLUE}Sweet!\nI'm glad you got it champ!\pI'm sure {COLOR GREEN}Professor Cypress{COLOR BLUE} would love to see that book.\pYou should definitely drop off that book at her lab in {COLOR GREEN}the southern side of the city{COLOR BLUE}.\pAnyways, I gotta go deal with some business stuff -- yadda yadda you know the boring old stuff.\pI'll see you on the flip side!"), MSGBOX_AUTOCLOSE)
	} else {
		msgbox(format("Lars: {COLOR BLUE}Ah you jokester!\pI know you got that book!\pI'm sure {COLOR GREEN}Professor Cypress{COLOR BLUE} would love to see that book.\pYou should definitely drop off that book at her lab in {COLOR GREEN}the southern side of the city{COLOR BLUE}.\pAnyways, I gotta go deal with some business stuff -- yadda yadda you know the boring old stuff.\pI'll see you on the flip side!"), MSGBOX_AUTOCLOSE)
	}
	specialvar(VAR_RESULT, GetPlayerFacingDirection)
	if (var(VAR_RESULT) == DIR_NORTH) {
		applymovement(4, MystCity_PokemonCenter_LarsEvent_Movement_0)
		waitmovement(0)
	} elif (var(VAR_RESULT) == DIR_WEST) {
		applymovement(4, MystCity_PokemonCenter_LarsEvent_Movement_1)
		waitmovement(0)
	} elif (var(VAR_RESULT) == DIR_EAST) {
		applymovement(4, MystCity_PokemonCenter_LarsEvent_Movement_1)
		waitmovement(0)
	}
	removeobject(4)
	playse(SE_EXIT)
	waitse
	delay(10)
	applymovement(5, Common_Movement_WalkInPlaceDown)
	waitmovement(0)
	msgbox(format("Grunt: {COLOR BLUE}So {COLOR GREEN}Cypress' Lab{COLOR BLUE} is where the {COLOR GREEN}Dorimtome{COLOR BLUE} will be..."), MSGBOX_AUTOCLOSE)
	delay(10)
	applymovement(5, MystCity_PokemonCenter_LarsEvent_Movement_2)
	waitmovement(0)
	removeobject(5)
	playse(SE_EXIT)
	waitse
	setflag(FLAG_MYSTCITY_POKEMONCENTER_LARSEVENT)
	setvar(VAR_CYPRESS_PROTO_EVENT_STATE, 1)
	setflag(FLAG_BAYPORTCITY_LARSHOUSE_LARSEVENT)
	release
	end
}
movement MystCity_PokemonCenter_LarsEvent_Movement_0 {
	walk_left
	walk_down * 2
	walk_right
	walk_down * 2
}
movement MystCity_PokemonCenter_LarsEvent_Movement_1 {
	walk_down * 4
}
movement MystCity_PokemonCenter_LarsEvent_Movement_2 {
	walk_fast_down * 2
	walk_fast_left * 3
	walk_fast_down * 2
	walk_fast_left * 2
	walk_fast_down * 2
}
script MystCity_PokemonCenter_Object_Grunt {
	lock
	faceplayer
	msgbox(format("Grunt: {COLOR BLUE}Hmmm?\pDon't talk to me!\pI'm...\nummm...\pLooking at the region map!"), MSGBOX_AUTOCLOSE)
	applymovement(5, Common_Movement_WalkInPlaceUp)
	waitmovement(0)
	release
	end
}