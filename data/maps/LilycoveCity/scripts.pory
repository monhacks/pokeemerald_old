mapscripts LilycoveCity_MapScripts {
	MAP_SCRIPT_ON_TRANSITION {
		setflag(FLAG_VISITED_LILYCOVE_CITY)
		if (var(VAR_PROTO_HIDEOUT) == 2) {
			setobjectxyperm(LARS_2, 32, 32)
			setobjectxyperm(MOM, 31, 32)
		}
		if (var(VAR_CYPRESS_PROTO_EVENT_STATE) == 5) {
			setobjectxyperm(SKIP, 15, 42)
			setobjectxyperm(LARS, 15, 39)
		}
	}
	MAP_SCRIPT_ON_FRAME_TABLE [
		VAR_PROTO_HIDEOUT, 2: MystCity_Mapscript_ExitProtoHideout
	]
}

